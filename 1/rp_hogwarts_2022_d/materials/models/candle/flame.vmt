"Unlitgeneric"
{
	
"$baseTexture" "models/candle/flame"
"$surfaceprop" "Default"
"$nodecal" 1
"$translucent" 1
"$additive" 1s
"$nocull" 1
$selfillum 1
$selfillummask "models/candle/candle_si"

	$emissiveblendenabled 0
   	$emissiveBlendBaseTexture "models/candle/candle_si"
